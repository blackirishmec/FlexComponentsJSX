		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-hooks": "^5.2.0",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"knip": "^5.46.0",
		"typescript": "^5.8.2",
		"vite": "^6.2.2",
		"vitest": "^3.0.9"
	},
	"dependencies": {
		"clsx": "^2.1.1",
